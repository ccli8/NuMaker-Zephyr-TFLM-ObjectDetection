#include <zephyr/dt-bindings/memory-attr/memory-attr-arm.h>

&npu0 {
	status = "okay";
};

/* HyperRAM */
&gpiog {
	status = "okay";
};
&gpioh {
	status = "okay";
};
&gpioj {
	status = "okay";
};

/* CCAP */
&gpiod {
	status = "okay";
